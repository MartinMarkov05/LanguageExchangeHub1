
<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LanguageExchangeHub1</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LanguageExchangeHub1.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">

                <div class="leftElements">



                    <div class="dropdown">
                        <button class="dropbtn" onclick="showMenuDropdownContent()">

                            <svg xmlns="http://www.w3.org/2000/svg" width="50" height="50" viewBox="0 0 50 50" fill="none">
                                <path d="M12.5 16.6667H37.5M12.5 25H37.5M12.5 33.3333H37.5" stroke="#111111" stroke-width="4.16667" stroke-linecap="round" stroke-linejoin="round" />
                            </svg>

                        </button>
                        <div id="menuDropdown" class="dropdown-content">
                            <a asp-controller="Home" asp-action="Index">Home</a>
                            <a asp-controller="Home" asp-action="Privacy">Privacy</a>
                            <a href="#">Assignements</a>
                            <a href="#">Chats</a>
                            <a href="#">Files</a>
                        </div>
                    </div>
                </div>

                <div class="title">
                    LanguageExchangeHub
                </div>

                <div>
                    @{
                        if (User.Identity.IsAuthenticated)
                        {
                            <a asp-controller="Identity" asp-action="Logout">Log out</a>
                        }
                    }

